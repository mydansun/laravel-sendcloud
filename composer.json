{
    "name": "mydansun/laravel-sendcloud",
    "description": "Sendcloud mail driver for Laravel",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Daniel Sun",
            "email": "mydansun@gmail.com"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "php": ">=7.1.0",
        "guzzlehttp/guzzle": "~6.0",
        "illuminate/support": "5.5.x|5.6.x|5.7.x|5.8.x",
        "illuminate/mail": "5.5.x|5.6.x|5.7.x|5.8.x"
    },
    "autoload": {
        "psr-4": {
            "Mydansun\\Sendcloud\\": "src/Sendcloud/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Mydansun\\Sendcloud\\Providers\\ServiceProvider"
            ],
            "aliases": {
                "Sendcloud": "Mydansun\\Sendcloud\\Facades\\Sendcloud"
            }
        }
    }
}
